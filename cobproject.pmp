		$this->BuildSearchSql($sWhere, $this->email4, $Default, FALSE); // email4

		// Set up search parm
		if (!$Default && $sWhere <> "" && in_array($this->Command, array("", "reset", "resetall"))) {
			$this->Command = "search";
