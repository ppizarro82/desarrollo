		{
			uluru = {lat: -17.977804381068, lng: -67.10541606250001};
			zoom = 12;
		}
		if (x.value == "8")
		{
			uluru = {lat: -14.827047472553783, lng: -64.89716410937501};
			zoom = 12;
		}
		if (x.value == "7")
		{
			uluru = {lat:-19.019654029686972, lng: -65.25971293750001};
			zoom = 12;
		}
		if (x.value == "9")
		{
