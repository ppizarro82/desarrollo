			$this->UpdateSort($this->nombres); // nombres
			$this->UpdateSort($this->paterno); // paterno
			$this->UpdateSort($this->materno); // materno
			$this->UpdateSort($this->marca); // marca
			$this->UpdateSort($this->modelo); // modelo
			$this->UpdateSort($this->codigo_placa); // codigo_placa
			$this->UpdateSort($this->anio); // anio
			$this->UpdateSort($this->adicional1); // adicional1
			$this->UpdateSort($this->adicional2); // adicional2
			$this->UpdateSort($this->adicional3); // adicional3
