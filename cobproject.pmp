	// Load sort order parameters
	function LoadSortOrder() {
		$sOrderBy = $this->getSessionOrderBy(); // Get ORDER BY from Session
		if ($sOrderBy == "") {
			if ($this->getSqlOrderBy() <> "") {
				$sOrderBy = $this->getSqlOrderBy();
				$this->setSessionOrderBy($sOrderBy);
			}
		}
	}

	// Reset command
	// - cmd=reset (Reset search parameters)
	// - cmd=resetall (Reset search and master/detail parameters)
	// - cmd=resetsort (Reset sort parameters)
	function ResetCmd() {

		// Check if reset command
		if (substr($this->Command,0,5) == "reset") {

			// Reset search criteria
			if ($this->Command == "reset" || $this->Command == "resetall")
				$this->ResetSearchParms();

			// Reset sorting order
			if ($this->Command == "resetsort") {
				$sOrderBy = "";
				$this->setSessionOrderBy($sOrderBy);
				$this->Id->setSort("");
				$this->id_fuente->setSort("");
				$this->id_gestion->setSort("");
				$this->id_tipobien->setSort("");
				$this->tipo_documento->setSort("");
				$this->no_documento->setSort("");
				$this->nombres->setSort("");
				$this->paterno->setSort("");
				$this->materno->setSort("");
				$this->marca->setSort("");
				$this->modelo->setSort("");
				$this->codigo_placa->setSort("");
				$this->anio->setSort("");
				$this->adicional1->setSort("");
				$this->adicional2->setSort("");
				$this->adicional3->setSort("");
				$this->adicional4->setSort("");
			}

			// Reset start position
			$this->StartRec = 1;
			$this->setStartRecordNumber($this->StartRec);
		}
	}

