	$bienes_list->StopRec = $bienes->GridAddRowCount;
}

// Initialize aggregate
$bienes->RowType = EW_ROWTYPE_AGGREGATEINIT;
$bienes->ResetAttrs();
$bienes_list->RenderRow();
while ($bienes_list->RecCnt < $bienes_list->StopRec) {
	$bienes_list->RecCnt++;
