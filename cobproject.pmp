		$bienes_list->RowCnt++;

		// Set up key count
		$bienes_list->KeyCount = $bienes_list->RowIndex;

		// Init row class and style
		$bienes->ResetAttrs();
		$bienes->CssClass = "";
		if ($bienes->CurrentAction == "gridadd") {
