		$this->nombres->setDbValue($row['nombres']);
		$this->paterno->setDbValue($row['paterno']);
		$this->materno->setDbValue($row['materno']);
		$this->marca->setDbValue($row['marca']);
		$this->modelo->setDbValue($row['modelo']);
		$this->codigo_placa->setDbValue($row['codigo_placa']);
		$this->anio->setDbValue($row['anio']);
		$this->adicional1->setDbValue($row['adicional1']);
		$this->adicional2->setDbValue($row['adicional2']);
		$this->adicional3->setDbValue($row['adicional3']);
		$this->adicional4->setDbValue($row['adicional4']);
	}

